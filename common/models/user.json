{
  "name": "user",
  "plural": "users",
  "base": "User",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "string",
      "length": 36,
      "defaultFn": "uuidv4",
      "id": true
    },
    "realm": {
      "type": "string",
      "length": 128
    },
    "username": {
      "type": "string",
      "length": 128
    },
    "password": {
      "type": "string",
      "required": true,
      "length": 256
    },
    "email": {
      "type": "string",
      "required": true,
      "length": 256
    },
    "emailVerified": "boolean",
    "verificationToken": {
      "type": "string",
      "length": 128
    }
  },
  "validations": [],
  "relations": {
    "devices": {
      "type": "hasMany",
      "model": "device",
      "foreignKey": "userId"
    },
    "accessTokens": {
      "type": "hasMany",
      "model": "accessToken",
      "foreignKey": "userId"
    },
    "identities": {
      "type": "hasMany",
      "model": "userIdentity",
      "foreignKey": "userId"
    },
    "credentials": {
      "type": "hasMany",
      "model": "userCredential",
      "foreignKey": "userId"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    }
  ],
  "methods": {}
}
